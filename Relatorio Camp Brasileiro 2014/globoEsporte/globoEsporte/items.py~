import scrapy


class GloboEsporteItem(scrapy.Item):
    # define the fields for your item here like:
    # name = scrapy.Field()
    titulo_pagina = scrapy.Field()
    autor_comentario = scrapy.Field()
    texto_comentario = scrapy.Field()
    link_noticia = scrapy.Field()
